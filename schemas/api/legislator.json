{"description": "legislator API response",
 "type": "object",
 "properties": {
   "id": {"type": "string", "pattern": "^[A-Z]{2}L\\d{6}$"},
   "full_name": {"type": "string"},
   "first_name": {"type": "string"},
   "last_name": {"type": "string"},
   "middle_name": {"type": "string", "blank": true},
   "suffixes": {"type": "string", "blank": true},
   "active": {"type": "boolean"},
   "created_at": {"type": "datetime"},
   "updated_at": {"type": "datetime"},
   "roles": {"type": "array", "items": {"type": "object", "properties": {
     "chamber": {"type": "string", "enum": ["upper", "lower", "joint"],
         "optional": true},
     "state": {"type": "string", "pattern": "^[a-z]{2}$"},
     "term": {"type": "string"},
     "type": {"type": "string"},
     "committee": {"type": "string", "optional": true},
     "subcommittee": {"type": ["string", "null"], "optional": true},
     "district": {"type": "string", "optional": true},
     "party": {"type": "string", "optional": true},
     "start_date": {"type": ["datetime", "null"], "optional": true},
     "end_date": {"type": ["datetime", "null"], "optional": true}
   }}},
   "sources": {"type": "array", "items": {"type": "object", "properties": {
     "url": {"type": "string"},
     "retrieved": {"type": "datetime"}
   }}},
   "votesmart_id": {"type": ["null", "string"], "optional": true},
   "nimsp_candidate_id": {"type": ["string", "null"], "optional": true},
   "state": {"type": "string", "pattern": "^[a-z]{2}$",
     "optional": true},
   "district": {"type": "string", "optional": true},
   "chamber": {"type": "string", "optional": true},
   "party": {"type": "string", "optional": true},
   "photo_url": {"type": "string", "optional": true, "blank": true}
}}
